{
    "name": "movielandia",
    "version": "1.0.0",
    "imports": {
        "~/*": "./src/*.ts"
    },
    "scripts": {
        "dev": "npm run server",
        "build": "vite build",
        "preview": "vite preview --port 4000",
        "server": "tsx server/server.ts",
        "seed": "npx prisma migrate dev && tsx prisma/seed/seed.ts",
        "server:prod": "cross-env NODE_ENV=production tsx src/server/server.ts",
        "prod": "npm run build && npm run server:prod",
        "lint": "eslint . --max-warnings 0",
        "format": "prettier --write **/*.{js,jsx,ts,tsx,css,md,json,scss} --config ./.prettierrc"
    },
