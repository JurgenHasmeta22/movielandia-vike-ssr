        "@prisma/client": "^5.17.0",
        "@types/react-slick": "^0.23.13",
        "bcrypt": "^5.1.1",
        "cross-env": "^7.0.3",
        "cross-fetch": "^4.0.0",
        "date-fns": "^3.6.0",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "formik": "^2.4.6",
        "framer-motion": "^11.2.14",
        "material-react-table": "^2.13.0",
